<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import java.net.URL?>
<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="gui.controller.LeaderboardController">
    <children>
        <Label layoutX="200.0" layoutY="36.0" prefHeight="56.0" prefWidth="327.0" id="main_title" text="Leaderboard" />

        <Button layoutX="200.0" layoutY="360.0" mnemonicParsing="false" id="grey_button" onAction="#show_home_page" prefWidth="200.0" text="Back"/>
        <Button layoutX="200.0" layoutY="315.0" mnemonicParsing="false" id="grey_button" onAction="#show_main_screen" prefWidth="200.0" text="Logout"/>

    </children>
    <TableView fx:id="tableView" depthTest="DISABLE" layoutX="180.0" prefHeight="400.0" prefWidth="300.0"  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="179.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="-1.0">
        <columns>
            <TableColumn fx:id="username" editable="false" prefWidth="160.0" sortable="false" text="Name" />
            <TableColumn fx:id="score" editable="false" prefWidth="173.0" text="Score" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
</AnchorPane>